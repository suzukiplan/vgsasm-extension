{
    "publisher": "suzukiplan",
    "name": "vgsasm",
    "version": "2024.11.9",
    "license": "GPL-3.0-only",
    "repository": {
        "type": "git",
        "url": "https://github.com/suzukiplan/vgsasm-extension"
    },
    "engines": {
        "vscode": "^1.33.1"
    },
    "main": "./extension.js",
    "activationEvents": [
        "onLanguage:vgsasm"
    ],
    "contributes": {
        "languages": [
            {
                "id": "vgsasm",
                "extensions": [
                    ".asm",
                    ".inc",
                    ".z80",
                    ".vgs"
                ]
            }
        ],
        "grammars": [
            {
                "language": "vgsasm",
                "scopeName": "source.vgsasm",
                "path": "./vgsasm.tmLanguage.json"
            }
        ]
    }
}